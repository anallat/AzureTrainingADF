{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "PW-DataFactory-MB"
		},
		"Azure_SQLDB_PWAzureTraining_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Azure_SQLDB_PWAzureTraining'"
		},
		"BlobStorage_azurestoragemb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorage_azurestoragemb'"
		},
		"OnPrem_SQL_PWAzureTrainingOnPrem_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OnPrem_SQL_PWAzureTrainingOnPrem'"
		},
		"DestinationDataset_ib6_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "dbo.emp"
		},
		"DestinationDataset_ib6_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "pw-container/onpremsqlserver"
		},
		"SourceDataset_9cg_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "inputEmp_tq.txt"
		},
		"SourceDataset_9cg_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "pw-container/Demo 05"
		},
		"azurestoragemb_blob_input_tq_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "inputEmp_tq.txt"
		},
		"azurestoragemb_blob_input_tq_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "pw-container/Demo 05"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Azure_SQLDB_PWAzureTraining_connectionString": {
						"value": "[parameters('Azure_SQLDB_PWAzureTraining_connectionString')]"
					},
					"BlobStorage_azurestoragemb_connectionString": {
						"value": "[parameters('BlobStorage_azurestoragemb_connectionString')]"
					},
					"OnPrem_SQL_PWAzureTrainingOnPrem_connectionString": {
						"value": "[parameters('OnPrem_SQL_PWAzureTrainingOnPrem_connectionString')]"
					},
					"DestinationDataset_ib6_properties_typeProperties_fileName": {
						"value": "[parameters('DestinationDataset_ib6_properties_typeProperties_fileName')]"
					},
					"DestinationDataset_ib6_properties_typeProperties_folderPath": {
						"value": "[parameters('DestinationDataset_ib6_properties_typeProperties_folderPath')]"
					},
					"SourceDataset_9cg_properties_typeProperties_fileName": {
						"value": "[parameters('SourceDataset_9cg_properties_typeProperties_fileName')]"
					},
					"SourceDataset_9cg_properties_typeProperties_folderPath": {
						"value": "[parameters('SourceDataset_9cg_properties_typeProperties_folderPath')]"
					},
					"azurestoragemb_blob_input_tq_properties_typeProperties_fileName": {
						"value": "[parameters('azurestoragemb_blob_input_tq_properties_typeProperties_fileName')]"
					},
					"azurestoragemb_blob_input_tq_properties_typeProperties_folderPath": {
						"value": "[parameters('azurestoragemb_blob_input_tq_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}